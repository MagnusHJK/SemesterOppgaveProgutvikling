<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="paneAdmin" prefHeight="750.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.controllers.Admin.ControllerAdmin">
    <children>
        <TabPane fx:id="tabpaneAdmin" prefHeight="683.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
            <Tab fx:id="tabOversikt" text="Adminastriv Oversikt">
                <fx:include fx:id="paneAdminOversikt" source="sceneAdminOversikt.fxml" />
            </Tab>
                <Tab fx:id="tabLeggTil" text="Legg til">
                    <fx:include fx:id="paneAdminLeggTil" source="sceneAdminLeggTil.fxml" />
                </Tab>

                <Tab fx:id="tabEndre" text="Endre">
                    <fx:include fx:id="paneAdminEndre" source="sceneAdminEndre.fxml" />
                </Tab>

                <Tab fx:id="tabSlett" text="Slett">
                    <fx:include fx:id="paneAdminSlett" source="sceneAdminSlett.fxml" />
                </Tab>

                <Tab fx:id="tabSerialiser" text="Serialiser">
                    <fx:include fx:id="paneAdminSerialiser" source="sceneAdminSerialiser.fxml" />
                </Tab>
            <Tab fx:id="tabFilbehandling" text="Filbehandling">
                <fx:include fx:id="paneAdminFilbehandling" source="sceneAdminFilbehandling.fxml" />
            </Tab>

            </tabs>
        </TabPane>
        <Button fx:id="btnHjemSide" layoutX="424.0" layoutY="683.0" mnemonicParsing="false" onAction="#actionHjemSide" text="Hjem" />
    </children>
</AnchorPane>
